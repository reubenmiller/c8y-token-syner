{
  "apiVersion": "v2",
  "name": "c8y-token-syner",
  "version": "0.0.2-SNAPSHOT",
  "provider": {
    "name": "thin-edge.io",
    "domain": "https://thin-edge.io",
    "support": "support@thin-edge.io"
  },
  "isolation": "PER_TENANT",
  "scale": "NONE",
  "replicas": 1,
  "requiredRoles": [
    "ROLE_INVENTORY_READ",
    "ROLE_INVENTORY_CREATE",
    "ROLE_INVENTORY_ADMIN",
    "ROLE_IDENTITY_READ",
    "ROLE_IDENTITY_ADMIN",
    "ROLE_USER_MANAGEMENT_READ",
    "ROLE_AUDIT_READ",
    "ROLE_AUDIT_ADMIN",
    "ROLE_EVENT_READ",
    "ROLE_EVENT_ADMIN",
    "ROLE_TENANT_ADMIN",
    "ROLE_DEVICE_CONTROL_ADMIN"
  ],
  "roles": [
    "ROLE_TOKEN_TRIAL_REQUEST_READ",
    "ROLE_TOKEN_TRIAL_REQUEST_CREATE",
    "ROLE_TOKEN_TRIAL_REQUEST_ADMIN"
  ],
  "resources": {
    "cpu": "0.5",
    "memory": "256Mi"
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/health",
      "port": 80
    },
    "initialDelaySeconds": 30,
    "periodSeconds": 120
  },
  "readinessProbe": {
    "httpGet": {
      "path": "/health",
      "port": 80

    },
    "initialDelaySeconds": 20,
    "periodSeconds": 15
  }
}